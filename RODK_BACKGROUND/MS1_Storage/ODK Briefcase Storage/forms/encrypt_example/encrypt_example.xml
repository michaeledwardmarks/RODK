<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head><!-- ODK Aggregate upload time: 2018-04-27T12:28:56.123+0000 on https://roberts-beta-001.odk.lshtm.ac.uk/roberts-beta-001 -->
    <h:title>encrypt_example</h:title>
    <model>
      <submission base64RsaPublicKey="" method="form-data-post"/>
      <instance>
        <tutorial_encrypted id="encrypt_example">
          <name/>
          <age/>
          <picture/>
          <has_children/>
          <gps/>
          <web_browsers/>
          <meta>
            <instanceID/>
          </meta>
        </tutorial_encrypted>
      </instance>
      <bind nodeset="/tutorial_encrypted/name" type="string"/>
      <bind nodeset="/tutorial_encrypted/age" type="int"/>
      <bind nodeset="/tutorial_encrypted/picture" type="binary"/>
      <bind nodeset="/tutorial_encrypted/has_children" type="select1"/>
      <bind nodeset="/tutorial_encrypted/gps" type="geopoint"/>
      <bind nodeset="/tutorial_encrypted/web_browsers" type="select"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/tutorial_encrypted/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/tutorial_encrypted/name">
      <label>1. What is your name?</label>
    </input>
    <input ref="/tutorial_encrypted/age">
      <label>2. How old are you?</label>
    </input>
    <upload mediatype="image/*" ref="/tutorial_encrypted/picture">
      <label>3. May I take your picture?</label>
    </upload>
    <select1 ref="/tutorial_encrypted/has_children">
      <label>4. Do you have any children?</label>
      <item>
        <label>no</label>
        <value>0</value>
      </item>
      <item>
        <label>yes</label>
        <value>1</value>
      </item>
    </select1>
    <input ref="/tutorial_encrypted/gps">
      <label>5. Record your GPS coordinates.</label>
      <hint>Make sure the precision is less than 25 meters</hint>
    </input>
    <select ref="/tutorial_encrypted/web_browsers">
      <label>6. What web browsers do you use?</label>
      <hint>Select all that apply</hint>
      <item>
        <label>Mozilla Firefox</label>
        <value>firefox</value>
      </item>
      <item>
        <label>Google Chrome</label>
        <value>chrome</value>
      </item>
      <item>
        <label>Internet Explorer</label>
        <value>ie</value>
      </item>
      <item>
        <label>Safari</label>
        <value>safari</value>
      </item>
    </select>
  </h:body>
</h:html>
